<html>
	<head><title>log on</title></head>
	<body>
		$!msg.getErrors()
		<form action="logon" method="post">
			<table>
				<tr>
					<td>name</td>
					<td><input type="text" name="userName" value="$!userName"></td>
				</tr>
				<tr>
					<td>password</td>
					<td><input type="password" name="password" value="$!password"></td>
				</tr>
			</table>
			<input type="submit" value="log on">
		</form>
	</body>
</html>