<html>
<head><title>Employee List</title></head>
<body>
$link
<table>
<tbody>
<tr><th>empno</th><th>name</th><th>date</th><th></th></tr>
#foreach($emp in $ret)
<tr>
	<td>$emp.empno</td>
	<td>$emp.name</td>
	<td>$dateFormat.format($emp.date)</td>
	<td>
		<a href="$command.employee.delete($emp.empno)">delete</a>
		<a href="$command.employee.updateForm($emp.empno)">update</a>
	</td>
</tr>
#end
</tbody>
</table>
#set($args = [])
<a href="$command.employee.addForm()">Add Employee</a>
</body>
</html>