<html>
<head><title>Employee List</title></head>
<body>
<table>
<tbody>
<tr><th>empno</th><th>name</th><th>date</th><th></th></tr>
#foreach($emp in $employees)
<tr>
	<td>$emp.empno</td>
	<td>$emp.name</td>
	<td>$dateFormat.format($emp.date)</td>
	<td>
		#if($link.authorized('employee','delete'))
		<a href="$link.employee.delete($emp.empno)">delete</a>
		#end
		<a href="$link.employee.updateForm($emp.empno)">update</a>
	</td>
</tr>
#end
</tbody>
</table>
#set($args = [])
<a href="$link.employee.addForm()">Add Employee</a>
</body>
</html>